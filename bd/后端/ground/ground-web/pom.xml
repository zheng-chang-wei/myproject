		<groupId>com.hirain.phm.bd</groupId>
		<artifactId>ground-life</artifactId>
	</dependency>
	<dependency>
		<groupId>com.hirain.phm.bd</groupId>
		<artifactId>ground-subhealth</artifactId>
	</dependency>
	<dependency>
		<groupId>com.hirain.phm.bd</groupId>
		<artifactId>ground-repair</artifactId>
	</dependency>
	<dependency>
		<groupId>com.hirain.phm.bd</groupId>
		<artifactId>ground-log</artifactId>
	</dependency>
